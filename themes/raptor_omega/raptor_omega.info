name = RAPTOR Omega
description = Theme for the RAPTOR application
base theme = omega
screenshot = screenshot.png
engine = phptemplate
core = 7.x

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = css/raptor-global.css
stylesheets[all][] = css/raptor-omega.normalize.css
stylesheets[all][] = css/raptor-omega.hacks.css
stylesheets[all][] = css/raptor-omega.styles.css
stylesheets[all][] = css/raptor-omega.no-query.css
stylesheets[all][] = css/jquery-ui-1.10.3.min.css
stylesheets[all][] = css/raptor-charts.css
stylesheets[all][] = css/jquery.dataTables.css
stylesheets[all][] = css/jquery.timepicker.css

; ========================================
; Scripts
; ========================================
scripts[] = js/jquery.noconflict.js
scripts[] = js/jquery-1.11.1.min.js
scripts[] = js/jquery.cookie.js
scripts[] = js/raptor-omega.behaviors.js
scripts[] = js/jquery.dataTables.min.js
scripts[] = js/jquery.timepicker.js
scripts[] = js/jquery.sticky.js
