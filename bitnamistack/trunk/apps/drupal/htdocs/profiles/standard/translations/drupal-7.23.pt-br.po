msgstr "Baixando %project"
msgid "Failed to download %project from %url"
msgstr "Falha ao baixar %project de %url"
msgid "Includes:"
msgstr "Inclui:"
msgid "Enabled: %includes"
msgstr "Ativado: %includes"
msgid "Disabled: %disabled"
msgstr "Desativado: %disabled"
