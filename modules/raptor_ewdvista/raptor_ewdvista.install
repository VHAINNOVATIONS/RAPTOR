function raptor_ewdvista_uninstall()
{
    $t = get_t();
    drupal_set_message($t('Uninstalled raptor_ewdvista'));
}

/**
 * Create indexes and insert baseline rows into tables
 * @return status
 */
function raptor_ewdvista_install()
{
    $t = get_t();
    try
    {
        drupal_set_message($t('Installed raptor_ewdvista'));
    } catch (\Exception $ex) {
        drupal_set_message($t('Failed install of raptor_ewdvista because ').$ex->getMessage(),'error');
    }
}

    